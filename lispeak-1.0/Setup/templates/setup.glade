                <child>
                  <object class="GtkLabel" id="lblDependsDesc">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">The following dependencies are missing from your computer.
You can click 'Install' to automatically install them or you can do this manually after.</property>
                    <property name="justify">center</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
                <child>
